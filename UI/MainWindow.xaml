<Window x:Class="UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ViewModels="clr-namespace:UI.ViewModels"
        Title="MainWindow"
        Height="350"
        Width="525">
	<Window.Resources>
		<ViewModels:ViewModelLocator x:Key="ViewModelLocator" />
	</Window.Resources>
	<Window.DataContext>
		<Binding>
			<Binding.Path >ViewModels[MainWindowViewModel]</Binding.Path>
			<Binding.Source>
				<StaticResource ResourceKey="ViewModelLocator" />
			</Binding.Source>
		</Binding>
	</Window.DataContext>
	<Grid DataContext="{Binding Path=Queue,IsAsync=True}">
		<ListBox ItemsSource="{Binding Path=Slots}" />
	</Grid>
</Window>