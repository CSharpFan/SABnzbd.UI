<Window x:Class="UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ViewModels="clr-namespace:UI.ViewModels"
        Title="MainWindow"
        Height="350"
		DataContext="{Binding Path=MainWindowViewModel.Queue, Source={StaticResource ViewModelLocator}}"
        Width="525">
	<Grid>
		<ListBox ItemsSource="{Binding Path=Slots}" BorderThickness="1" BorderBrush="#FF0034D1"/>
	</Grid>
</Window>